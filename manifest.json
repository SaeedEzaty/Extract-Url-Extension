{
  "manifest_version": 3,
  "name": "Auto Path Extractor",
  "description": "به طور خودکار تمام مسیرهای منحصر به فرد را از وب‌سایت‌های بازدید شده استخراج می‌کند.",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ]
}

